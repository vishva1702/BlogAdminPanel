@{
    ViewData["Title"] = "Users";
}

<h1>@ViewData["Title"]</h1>
<p>List of users will appear here.</p>

<!-- Example table of users -->
<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Email</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        <!-- Example loop to display users -->
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.Name</td>
                <td>@user.Email</td>
                <td>
                    <a href="@Url.Action("Edit", "User", new { id = user.Id })">Edit</a> |
                    <a href="@Url.Action("Delete", "User", new { id = user.Id })">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
